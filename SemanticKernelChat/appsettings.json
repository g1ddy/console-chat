{
  "McpServers": {
    "McpToDoList": {
      "TransportType": "sse",
      "Command": "https://localhost:7283/sse",
      // https://github.com/microsoft/mcp-dotnet-samples/tree/main/todo-list
      // Disabled by default since you need to be running server
      "disabled": true,
      "autoApprove": []
    },
    "McpServer": {
      "TransportType": "stdio",
      "Command": "dotnet",
      "args": [
        "run",
        "--project",
        "../../../../McpServer",
        "--no-build"
      ]
    }
  }
}
