{
  "McpServers": {
    "atlassian": {
      "transportType": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.atlassian.com/v1/sse"
      ],
      "disabled": true,
      "autoApprove": []
    },
    "McpToDoList": {
      "transportType": "sse",
      "command": "https://localhost:7283/sse",
      // https://github.com/microsoft/mcp-dotnet-samples/tree/main/todo-list
      // Disabled by default since you need to be running server
      "disabled": true,
      "autoApprove": []
    },
    "McpServer": {
      "transportType": "stdio",
      "command": "dotnet",
      "args": [
        "run",
        "--project",
        "../../../../McpServer",
        "--no-build"
      ]
    }
    ,
    "RaindropServer": {
      "transportType": "stdio",
      "command": "dotnet",
      "args": [
        "run",
        "--project",
        "../../../../RaindropServer",
        "--no-build"
      ]
    }
  },
  "Raindrop": {
    "ApiToken": "",
    "BaseUrl": "https://api.raindrop.io/rest/v1"
  }
}
